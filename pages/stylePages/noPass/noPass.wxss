@import '../hasMatch/hasMatch';
@import '/templates/footerItem/footerItem';
.noPass_list{
  width:100%;
  white-space: nowrap;
}
.noPass_cell{
  margin:20rpx 20rpx 0; 
  background: #fff;

}
.noPass_head{
  height:88rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  padding-right:20rpx;
}
.shop_sel{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  width:90rpx;
}
.sel_icon{
  width: 34rpx;
  height:34rpx;
}
.noPass_shop{
  padding:20rpx;  
  height:172rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: flex-start;
  align-content: stretch;
}
.shop_name{
  width:100%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
 .shop_people{
   width: 100%;
  height:30rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items:center;
  align-content: flex-start;
}
.shop_people>image{
  width:22rpx;
  height:24rpx;
  margin-right:10rpx;
}
.shop_address{
  height:30rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items:center;
  align-content: flex-start;
}
.shop_address>image{
  width:20rpx;
  height:28rpx;
  margin-right:10rpx;
} 
.style_list .style_cell:last-child{
  border:none;
}
.style_cell{
  height:168rpx;
}
.style_info{
  height:100%;
}
.style_img{
  width:120rpx;
  height:120rpx;
  margin-right:20rpx;
}

