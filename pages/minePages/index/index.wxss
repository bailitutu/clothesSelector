.userInfo{
  width:100%;
  text-align: center;
  
}
.userInfo .userHead{
  width:136rpx;
  height:135rpx;
  margin:54rpx auto 0;
  border-radius: 50%;
  overflow:hidden;
}
.labelList{
  width:100%;
  height:auto;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  align-content: space-around;
  flex-wrap: wrap;
  padding:0 40rpx;
}
.labelList .label-cell{
  font-size:22rpx;
  height:44rpx;
  line-height: 40rpx;
  border:1px solid #e2e2e2;
  border-radius:8px;
  padding:0 10rpx;
  white-space: nowrap;
  margin-top:24rpx;
  margin-left:24rpx;
}

/* 二分展示 */
.full_two_item{
  width:100%;
  height:140rpx;
  background: #fff;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  padding:20rpx 0 ;
}
.full_two_item .full_two_cell{
  width:50%;
  height:100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
}
.full_two_item .full_two_cell:first-child{
  border-right:1px solid #e2e2e2;
}

/* 进入标签 */

.go_item{
  width:100%;
  background: #fff;
  height:90rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  padding:0 40rpx;
}
.go_item>image{
  width:16rpx;
  height:30rpx;
}