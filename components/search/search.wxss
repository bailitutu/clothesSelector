.search_compo{
  height:76rpx;
  padding:0 30rpx;
  background: #03A863;
  width:100%;
}
.search_compo .search_inner{
  width:100%;
  height:100%;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  align-content: center;
  border-bottom:1px solid #fff;
  
}
.fc-fe{
  color:#fefefe;
}

.search_inner .search_btn{
  width:80rpx;
  height:100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.search_inner .search_btn>image{
  width: 40rpx;
  height: 40rpx;
}
.fc-fff{
  color:#fff;
}
.fs-32{
  font-size:32rpx;
}
.fs-28{
  font-size:28rpx;
}